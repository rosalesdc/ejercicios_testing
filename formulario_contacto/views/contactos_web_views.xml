<?xml version="1.0" encoding="utf-8" ?>
<odoo>
  <data>
    <record id="view_contactos_web_form" model="ir.ui.view">
      <field name="name">view.contactos.web.form</field>
      <field name="model">contactos.web</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <div class="oe_title">
              <h1>
                Contactos Web
              </h1>
            </div>
            <group>
              <field name="name" />
              <field name="correo" />
              <field name="edad" />
              <field name="usuario_id" />
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="contacto_web_action_window" model="ir.actions.act_window">
      <field name="name">contacto_web</field>
      <field name="res_model">contactos.web</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="master_contactos_web" name="Contactos web" sequence="50" />
    <menuitem id="personas_menuitem" name="ContactosWeb" action="contacto_web_action_window" parent="master_contactos_web" sequence="10" />
  </data>
</odoo>